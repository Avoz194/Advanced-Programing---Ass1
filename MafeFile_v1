#All Targets
all: project

#Tool invocations
project: bin/Session.o bin/Agent.o bin/Graph.o bin/Tree.o
		@echo 'Building target: project'
		@echo 'Invoking: C++ Linker'
		g++ -o bin/project bin/Session.o bin/Agent.o bin/Graph.o bin/Tree.o
		@echo 'Finished building target: project'
		@echo ' '

#Depends on the source and header files
bin/Session.o: src/Session.cpp
		g++ -g